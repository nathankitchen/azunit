{
    "manifestVersion": 1,
    "id": "azunit-test-runner",
    "name": "AzUnit Test Runner",
    "version": "0.0.1",
    "publisher": "nathan-kitchen",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools for building/releasing with Fabrikam. Includes one build/release task.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/azunit.png"        
    },
    "files": [
        {
            "path": "packages/azunit-ext-devops/dist"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ]
        }
    ]
}